{
    "tempParams":{
        "boot":{
            "loader":{
                "efi":{"canTouchEfiVariables":true},
                "systemd-boot":{"enable":true}
            }
        },
        "console":{"keyMap":"fr"},
        "hardware":{
            "monitors":{
                "active":"mastodant-3-screens"
            },
            "pulseaudio":{
                "enable":false
            }
        },
        "i18n":{
            "defaultLocale":"en_US.UTF-8",
            "extraLocaleSettings":{
                "LC_ADDRESS":"fr_FR.UTF-8",
                "LC_IDENTIFICATION":"fr_FR.UTF-8",
                "LC_MEASUREMENT":"fr_FR.UTF-8",
                "LC_MONETARY":"fr_FR.UTF-8",
                "LC_NAME":"fr_FR.UTF-8",
                "LC_NUMERIC":"fr_FR.UTF-8",
                "LC_PAPER":"fr_FR.UTF-8",
                "LC_TELEPHONE":"fr_FR.UTF-8",
                "LC_TIME":"fr_FR.UTF-8"
            }
        },
        "imports":["/nix/store/553327kq730k80bql0g79y7h0bx7zfr5-hardware-configuration.nix"],
        "networking":{
            "hostName":"karle",
            "networkmanager":{"enable":true}
        },
        "nix":{
            "settings":{
                "experimental-features":["nix-command","flakes"]
            }
        },
        "nixpkgs":{
            "config":{"allowUnfree":true}
        },
        "programs":{
            "firefox":{"enable":true},
            "hyprland":{"enable":true}
        },
        "security":{
            "rtkit":{"enable":true}
        },
        "services":{
            "pipewire":{
                "alsa":{
                    "enable":true,
                    "support32Bit":true
                },
                "enable":true,
                "pulse":{"enable":true}
            },
            "printing":{
                "enable":true
            },
            "xserver":{
                "desktopManager":{
                    "gnome":{"enable":true}
                },
                "displayManager":{
                    "gdm":{"enable":false},
                    "sddm":{"enable":true}
                },
                "enable":true,
                "xkb":{"layout":"fr","variant":""}
            }
        },
        "stylix":{"enable":true},
        "system":{"stateVersion":"24.11"},
        "time":{"timeZone":"Europe/Paris"},
        "users":{
            "users":{
                "karim":{
                    "description":"Karim",
                    "extraGroups":["networkmanager","wheel"],
                    "isNormalUser":true
                }
            }
        }
    },
    "user":"karim"
}